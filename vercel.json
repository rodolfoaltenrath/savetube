{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/main.py": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/health.py": {
      "maxDuration": 10,
      "memory": 128
    }
  },
  "rewrites": [
    { "source": "/metadata", "destination": "/api/main/metadata" },
    { "source": "/download-progress", "destination": "/api/main/download-progress" }
  ]
}
